uint8_t st;
uint8_t res;

res = 26*(1-st)*(1-st/2)*(1-st/3)*(1-st/4)*(1-st/5)*(1-st/6) +
	  65*st*(1-st/2)*(1-st/3)*(1-st/4)*(1-st/5)*(1-st/6)*6 +
	  234*st*(1-st)*(1-st/3)*(1-st/4)*(1-st/5)*(1-st/6)*(-15)/2 +
	  31*st*(1-st)*(1-st/2)*(1-st/4)*(1-st/5)*(1-st/6)*20/3 +
	  25*st*(1-st)*(1-st/2)*(1-st/3)*(1-st/5)*(1-st/6)*(-15)/4 +
	  25*st*(1-st)*(1-st/2)*(1-st/3)*(1-st/4)*(1-st/6)*6/5 +
	  25*st*(1-st)*(1-st/2)*(1-st/3)*(1-st/4)*(1-st/5)/(-6);
